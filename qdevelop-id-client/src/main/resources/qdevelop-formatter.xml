<?xml version="1.0" encoding="UTF-8"?>
<data-formatter>

<!-- ID自动生成参数插件，默认生成6位的ID，可以通过使用时指定digit来调整 -->
<param-formatter name="id-generate-parameter" class="cn.qdevelop.plugin.id.client.formatter.IDGenerateFormatter">
  <!-- 指定生成的参数key名称 -->
  <param-key>参数名</param-key>
  <!-- 指定唯一应用名称，一般写成表名即可 -->
  <app-id-name>应用ID名称</app-id-name>
  <!-- 非必填参数
  <wrapper>自定义内容模版，支持时间变量和ID变量；例： WX-{DATE}-{ID}</wrapper>
  <date-format>wrapper变量中{DATE}的时间格式化样式，默认：yyMMdd</date-format>
  <digit>需要几位数字，默认6位</digit>
  <buffer>一次性从服务端获取数量，默认一次取5条</buffer> 
  -->
</param-formatter>
</data-formatter>